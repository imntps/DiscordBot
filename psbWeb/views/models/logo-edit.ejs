<div id="logo-edit" uk-modal>
    <div class="uk-modal-dialog uk-modal-body">
        <h2 class="uk-modal-title">เปลี่ยนโลโก้</h2>
       
        <form class="uk-form-horizontal uk-margin-large" method="post" enctype="multipart/form-data" action="/logoedit">
            <div class="uk-margin">
                <div class="js-upload" uk-form-custom>
                    <input class="uk-input" id="form-horizontal-text" name="_id" type="hidden" value="<%= data._id %>">
                    <input class="uk-input" id="form-horizontal-text" name="teamname" type="hidden" value="<%= data.team %>">
                    <input type="file" multiple name="logo">
                    <button class="uk-button uk-button-default" type="button" tabindex="-1">เลือกรูปภาพ</button>
                </div>
            </div>

            <div class="uk-margin">
                <button class="uk-button uk-button-primary" type="submit">ยืนยัน</button>
                <button class="uk-button uk-button-default uk-modal-close" type="button">ยกเลิก</button>
            </div>
        </form>
    </div>
</div>