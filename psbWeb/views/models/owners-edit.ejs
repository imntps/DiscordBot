<% for(i = 1; i<=2; i++) { %>
    <div id="owner<%= i %>-edit" uk-modal>
        <div class="uk-modal-dialog uk-modal-body">
            <h2 class="uk-modal-title">เปลี่ยนดิสคอร์ดไอดี <%= i %></h2>
           
            <form class="uk-form-horizontal uk-margin-large" method="post" action="/owner<%= i %>edit">
                <div class="uk-margin">
                    <input class="uk-input" id="form-horizontal-text" name="_id" type="hidden" value="<%= data._id %>">
                    <input class="uk-input" id="form-horizontal-text" name="owner<%= i %>" type="text" placeholder="เปลี่ยนดิสคอร์ดไอดี <%= i %>">
                </div>
    
                <div class="uk-margin">
                    <button class="uk-button uk-button-primary" type="submit">ยืนยัน</button>
                    <button class="uk-button uk-button-default uk-modal-close" type="button">ยกเลิก</button>
                </div>
            </form>
        </div>
    </div>
<% } %>